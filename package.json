{
  "name": "webgrider",
  "version": "0.1.0",
  "author": "amirhouieh",
  "license": "MIT",
  "homepage": "https://github.com/amirhouieh/webgrider",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "module": "dist/webgrider.esm.js",
  "size-limit": [
    {
      "path": "dist/webgrider.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/webgrider.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^6.0.4",
    "size-limit": "^6.0.4",
    "tsdx": "^0.14.1"
  },
  "dependencies": {
    "puppeteer": "^10.4.0"
  }
}
